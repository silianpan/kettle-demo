  */

	String subjectCode = get(Fields.In, "科目编码").getString(r);

	// 判断科目编码是否等于合计行
	if ("合计".equals(subjectCode)) {
		get(Fields.Out, "科目编码").setValue(r, 0);
		get(Fields.Out, "科目名称").setValue(r, "合计");
	} else if (null != subjectCode &amp;&amp; !"".equals(subjectCode) &amp;&amp; subjectCode.lastIndexOf(".") != -1) {
		get(Fields.Out, "科目编码").setValue(r, subjectCode.substring(0, subjectCode.lastIndexOf(".")));
	}

  // Send the row on to the next step.
  putRow(data.outputRowMeta, r);

  return true;
}
